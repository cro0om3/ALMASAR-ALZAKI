# MCP Servers Environment Variables
# Copy this to .env.local and fill in your values

# DigitalOcean API Token
# Get from: https://cloud.digitalocean.com/account/api/tokens
DIGITALOCEAN_API_TOKEN=your_digitalocean_token_here

# Namecheap API Credentials
# Get from: https://www.namecheap.com/myaccount/api/
NAMECHEAP_API_USER=your_namecheap_username
NAMECHEAP_API_KEY=your_namecheap_api_key
NAMECHEAP_IP=your_ip_address
